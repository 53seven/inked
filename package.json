{
  "name": "nought",
  "version": "0.0.0",
  "description": "extendable and reusable d3 charts in ES6 classes",
  "keywords": [
    "d3",
    "plugin",
    "line chart"
  ],
  "license": "MIT",
  "main": "build/nought.js",
  "jsnext:main": "index.js",
  "homepage": "https://github.com/53seven/nought",
  "repository": {
    "type": "git",
    "url": "https://github.com/53seven/nought.git"
  },
  "watch": {
    "build": "{index,src/}*.js"
  },
  "scripts": {
    "build": "mkdir -p build && rollup -c rollup.config.js -o build/nought.js",
    "minify": "return 0;#uglifyjs build/nought.js -c -m -o build/nought.min.js",
    "archive": "rm -f build/nought.zip && zip -j build/nought.zip -- LICENSE README.md build/nought.js build/nought.min.js",
    "pretest": "npm run build",
    "test": "faucet `find test -name '*-test.js'`",
    "version": "npm run build && npm run minify && git add build/nought.js build/nought.min.js",
    "prepublish": "npm run test && npm run minify && npm run archive",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "d3-svg": "^0.2.0",
    "faucet": "0.0",
    "jsdom": "^8.1.0",
    "npm-watch": "^0.1.4",
    "rollup": "0.25.4",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.4.0",
    "tape": "4",
    "tape-catch": "^1.0.4",
    "uglify-js": "2"
  },
  "dependencies": {
    "d3-array": "^0.7.1",
    "d3-axis": "^0.3.0",
    "d3-interpolate": "^0.7.0",
    "d3-scale": "^0.6.1",
    "d3-selection": "^0.7.0",
    "d3-shape": "^0.5.1",
    "d3-transition": "^0.2.8",
    "lodash-es": "^4.11.1"
  }
}
